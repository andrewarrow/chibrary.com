RewriteEngine On

DirectoryIndex off

# list
RewriteRule ^/([a-zA-Z\-])$  /$1/index.html
RewriteRule ^/([a-zA-Z\-])/$ /$1/index.html
# year
RewriteRule ^/([a-zA-Z\-])/[0-9][0-9][0-9][0-9]$  /$1 [R=301,L]
RewriteRule ^/([a-zA-Z\-])/[0-9][0-9][0-9][0-9]/$ /$1 [R=301,L]
# month
RewriteRule ^/([a-zA-Z\-]/[0-9][0-9][0-9][0-9]/[0-9][0-9])$  /$1/index.html [R=301,L]
RewriteRule ^/([a-zA-Z\-]/[0-9][0-9][0-9][0-9]/[0-9][0-9])/$ /$1/index.html [R=301,L]

